import site.cli.build
import site.cli.news
import site.cli.packages
import site.cli.sponsors
import std.env

type async Main {
  fn async main {
    match env.arguments.get(0) {
      case Ok('build') or Error(_) -> build.run
      case Ok('sponsors') -> sponsors.run
      case Ok('packages') -> packages.run
      case Ok('news') -> news.run
      case Ok(v) -> panic('unknown command: ${v}')
    }
  }
}
